# MS SQL Configuration Example for Vanna MCP Server
# Copy this to .env and fill in your actual values

# Database Type - Change to mssql
DATABASE_TYPE=mssql

# MS SQL Server Configuration
MSSQL_SERVER=your-server.database.windows.net
MSSQL_DATABASE=your-database-name
MSSQL_USERNAME=your-username
MSSQL_PASSWORD=your-password

# Optional MS SQL Settings
MSSQL_DRIVER=ODBC Driver 17 for SQL Server
MSSQL_ENCRYPT=true
MSSQL_TRUST_SERVER_CERTIFICATE=false

# Supabase Configuration (Still needed for vector storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_DB_PASSWORD=your-postgres-password

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Multi-Tenant Configuration
ENABLE_MULTI_TENANT=true
TENANT_ID=zadley
ALLOWED_TENANTS=zadley,zadley_india,zadley_retail
STRICT_TENANT_ISOLATION=true
ENABLE_SHARED_KNOWLEDGE=true

# Vanna Schema Configuration
VANNA_SCHEMA=vannabq
VANNA_MODEL_NAME=mssql-assistant

# Access Control
ACCESS_CONTROL_MODE=whitelist
ACCESS_CONTROL_DATASETS=

# Query Settings
MANDATORY_QUERY_VALIDATION=true
MAX_QUERY_RESULTS=10000

# Logging
LOG_LEVEL=INFO
LOG_FILE=